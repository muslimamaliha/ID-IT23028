<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<VBox alignment="CENTER" spacing="15" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.LeaderboardController">
    <TableView fx:id="leaderboardTable" prefHeight="300" prefWidth="300">
        <columns>
            <TableColumn fx:id="nameColumn" text="Name" prefWidth="150"/>
            <TableColumn fx:id="scoreColumn" text="Score" prefWidth="150"/>
        </columns>
    </TableView>
    <Button text="Back" onAction="#handleBack"/>
</VBox>